{
  "name": "shoppingcartnodejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "npm run test-unit && npm run test-integration && npm run test-contract",
    "test-integration": "mocha --opts test/integration/mocha.opts test/integration/**/*.js",
